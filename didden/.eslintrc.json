{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["import"],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-unused-vars": "error",
    "import/order": [
      "error",
      {
        "groups": ["builtin", "external", "internal", "parent", "sibling", "index", ["unknown", "object"], "type"],
        "newlines-between": "always",
        "alphabetize": {
          "caseInsensitive": true
        }
      }
    ]
  }
}
